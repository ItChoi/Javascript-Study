<script type="text/javascript">
	// 숫자 타입
	var intNum = 10;
	var floatNum = 0.1;
	consoel.log("intNum::: " + typeof intNum);
	consoel.log("floatNum::: " + typeof floatNum);
	
	// 문자열 타입
	var singleQuoteStr = 'single quote string';
	var doubleQuoteStr = "double quote string";
	var singleChar = 'a';
	consoel.log("singleQuoteStr::: " + typeof singleQuoteStr);
	consoel.log("doubleQuoteStr::: " + typeof doubleQuoteStr);
	consoel.log("singleChar::: " + typeof singleChar);
	
	// 불린 타입
	var boolVAr = true;
	
	// undefined 타입
	var emptyVar;
	
	// null 타입
	var nullVar = null;
	

</script>


